grammar uk.ac.kcl.inf.mdd.tracerypp.TraceryPP with org.eclipse.xtext.common.Terminals

generate traceryPP "http://www.ac.uk/kcl/inf/mdd/tracerypp/TraceryPP"

Model:
	statements+=Statement*;
	
Statement:
	Title |
	Sentence
;
	

Title:
	start_symbol = StartSymbol  " can have values: " list = List
;

// color can have values: orange, blue, green...
StartSymbol:
	title = STRING
;

List:
	word = Word (("," | ", ") word = Word)*
;

Word:
	word = STRING
;

Sentence:
	
;


